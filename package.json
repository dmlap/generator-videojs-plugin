{
  "name": "generator-videojs-plugin",
  "version": "5.2.1",
  "description": "A Yeoman generator for Video.js plugins",
  "license": "Apache-2.0",
  "main": "generators/app/index.js",
  "engine": {
    "node": ">=4.8.0"
  },
  "repository": "videojs/generator-videojs-plugin",
  "author": {
    "name": "Pat O'Neill",
    "email": "pgoneill@gmail.com",
    "url": "https://github.com/misteroneill"
  },
  "vjsstandard": {
    "ignore": [
      "docs",
      "generators/app/templates"
    ]
  },
  "scripts": {
    "docs": "doctoc README.md docs/conventions.md",
    "lint": "vjsstandard",
    "precommit": "npm run docs && git add README.md docs/conventions.md",
    "prepush": "npm run lint",
    "pretest": "npm run lint",
    "test": "mocha test/*.test.js",
    "version": "node scripts/version.js"
  },
  "keywords": [
    "yeoman-generator",
    "videojs",
    "videojs-plugin"
  ],
  "dependencies": {
    "chalk": "^2.3.1",
    "fs-extra": "^5.0.0",
    "lodash": "^4.17.5",
    "rimraf": "^2.6.1",
    "semver": "^5.1.0",
    "tsmlj": "^1.0.0",
    "yeoman-generator": "^2.0.3"
  },
  "devDependencies": {
    "conventional-changelog-cli": "^1.3.1",
    "conventional-changelog-videojs": "^3.0.0",
    "doctoc": "^1.3.1",
    "husky": "^0.14.3",
    "mocha": "^5.0.1",
    "videojs-standard": "^6.0.0"
  }
}
